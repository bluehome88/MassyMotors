<div id="hilo-jobapps-main">

<div class="row">

	<div class="span4">
		<h2>Find by Date<i class="icon-search"></i></h2>
		<div class="arn-col-sec">
			  <div class="input-control text">
			  Application Date
			        <input type="text" id="appDate" placeholder="YYYY-MM-DD" />
			    </div>
		</div>
		<ul id="resDate" class="listview sublistview"></ul>
	</div>
	
	<div class="span4">
	
		<h2>Find by Details<i class="icon-search"></i></h2>
		<div class="arn-col-sec">
			  <div class="input-control text">
			  Application Details
			        <input type="text" id="appDetails" placeholder="Eg. Cashier, Gulf City, Full-Time" />
			    </div>
		</div>
		<ul id="resDetails" class="listview sublistview"></ul>
	</div>
	
	<div class="span4" id="jobapps-details"></div>

</div>

</div>